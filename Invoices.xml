select fk_facture As id, 
ROUND(sum(total_ht)-sum(buy_price_ht*qty),2) AS marg_prod,
ROUND((100*(sum(total_ht)-sum(buy_price_ht*qty)))/sum(total_ht),2) AS mark_rate
from llx_facturedet 
where  product_type = 0
group by fk_facture;</SQLText></PreSQL><PreSQL><SQLText>CREATE TEMPORARY TABLE margin_service (id INT(11), marg_serv DOUBLE(12,2), mark_rate DOUBLE(12,2))</SQLText></PreSQL><PreSQL><SQLText>INSERT INTO margin_service (id,marg_serv, mark_rate)
SELECT fk_facture As id, 
ROUND(sum(total_ht)-sum(buy_price_ht*qty),2) AS marg_serv,
ROUND((100*(sum(total_ht)-sum(buy_price_ht*qty)))/sum(total_ht),2) AS mark_rate
FROM  llx_facturedet 
WHERE  product_type = 1
GROUP BY fk_facture
HAVING  sum(total_ht) &gt; 0;</SQLText></PreSQL><PreSQL><SQLText>CREATE TEMPORARY TABLE total_margin (id INT(11), marg DOUBLE(12,2), mark_rate DOUBLE(12,2))</SQLText></PreSQL><PreSQL><SQLText>INSERT INTO total_margin (id,marg, mark_rate)
SELECT fk_facture As id, 
ROUND(sum(total_ht)-sum(buy_price_ht*qty),2) AS marg,
ROUND((100*(sum(total_ht)-sum(buy_price_ht*qty)))/sum(total_ht),2) AS mark_rate
FROM  llx_facturedet 
WHERE  1 = 1
GROUP BY fk_facture
HAVING  sum(total_ht) &gt; 0;</SQLText></PreSQL><PreSQL><SQLText>CREATE TEMPORARY TABLE total_avmargin (id INT(11), marg DOUBLE(12,2), mark_rate DOUBLE(12,2))</SQLText></PreSQL><PreSQL><SQLText>INSERT INTO total_avmargin (id,marg, mark_rate)
select fd.fk_facture As id, 
ROUND(sum(fd.total_ht)+sum(fd.buy_price_ht*qty),2) AS marg_prod,
ROUND((100*(sum(fd.total_ht)+sum(fd.buy_price_ht*fd.qty)))/sum(fd.total_ht),2) AS mark_rate
from llx_facturedet AS fd
LEFT JOIN llx_facture AS f ON f.rowid = fd.fk_facture 
where f.type = 2
group by fd.fk_facture;</SQLText></PreSQL><PreSQL><SQLText>CREATE TEMPORARY TABLE margin_prodht (id INT(11), ht DOUBLE(12,2))</SQLText></PreSQL><PreSQL><SQLText>INSERT INTO margin_prodht (id,ht)
